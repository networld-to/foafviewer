<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="to.networld.android.foafviewer"
      android:versionCode="3"
      android:versionName="0.10">
    <application 
    	android:icon="@drawable/foaf"
    	android:label="@string/app_name"
    	android:debuggable="false"
    >
        <activity android:name=".FOAFViewer" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <uses-library android:name="com.google.android.maps" />
        <activity android:name=".FOAFFriendListing" android:label="@string/friend_list"/>
        <activity android:name=".FOAFProfile" android:label="@string/foaf_profile"/>
        <activity android:name=".FOAFSettings" android:label="@string/settings"/>
    </application>
    <uses-sdk android:minSdkVersion="8" />


<uses-permission android:name="android.permission.INTERNET"/>
<uses-permission android:name="android.permission.WRITE_CONTACTS"/>
<uses-permission android:name="android.permission.WRITE_CALENDAR"/>
</manifest>
